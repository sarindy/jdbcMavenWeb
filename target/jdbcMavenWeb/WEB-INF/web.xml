<?xml version="1.0"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
                         "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
 <display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>ProcessLogin</servlet-name>
		<display-name>ProcessLogin</display-name>
		<description></description>
		<servlet-class>servlet.ProcessLogin</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AccountList</servlet-name>
		<display-name>AccountList</display-name>
		<description></description>
		<servlet-class>servlet.AccountList</servlet-class>
	</servlet>
		
	<servlet-mapping>
		<servlet-name>ProcessLogin</servlet-name>
		<url-pattern>/ProcessLogin</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AccountList</servlet-name>
		<url-pattern>/AccountList</url-pattern>
	</servlet-mapping>
	
</web-app>
